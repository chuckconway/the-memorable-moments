<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="TheMemorableMoments" namespace="TheMemorableMoments.Infrastructure.Data.DTOs" xmlns="urn:nhibernate-mapping-2.2">
  <class name="UploadBatch" table="UploadBatch" lazy="true" >
    <id name="BatchId">
      <generator class="identity" />
      <column name="BatchId" sql-type="uniqueidentifier" not-null="true" />
    </id>
    <property name="LocationName">
      <column name="LocationName" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Latitude">
      <column name="Latitude" sql-type="decimal" not-null="false" />
    </property>
    <property name="Longitude">
      <column name="Longitude" sql-type="decimal" not-null="false" />
    </property>
    <property name="Zoom">
      <column name="Zoom" sql-type="int" not-null="false" />
    </property>
    <property name="MapTypeId">
      <column name="MapTypeId" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Year">
      <column name="Year" sql-type="int" not-null="false" />
    </property>
    <property name="Month">
      <column name="Month" sql-type="int" not-null="false" />
    </property>
    <property name="Day">
      <column name="Day" sql-type="int" not-null="false" />
    </property>
    <property name="Albums">
      <column name="Albums" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Tags">
      <column name="Tags" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="MediaStatus">
      <column name="MediaStatus" sql-type="nvarchar" not-null="false" />
    </property>
  </class>
</hibernate-mapping>