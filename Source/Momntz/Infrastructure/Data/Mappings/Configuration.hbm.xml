<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="TheMemorableMoments" namespace="TheMemorableMoments.Infrastructure.Data.DTOs" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Configuration" table="Configuration" lazy="true" >
    <property name="ConfigurationId">
      <column name="ConfigurationId" sql-type="uniqueidentifier" not-null="true" />
    </property>
    <property name="SiteName">
      <column name="SiteName" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="SiteTagLine">
      <column name="SiteTagLine" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Membership">
      <column name="Membership" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="ThumbNailHeight">
      <column name="ThumbNailHeight" sql-type="smallint" not-null="true" />
    </property>
    <property name="ThumbNailWidth">
      <column name="ThumbNailWidth" sql-type="smallint" not-null="true" />
    </property>
    <property name="WebHeight">
      <column name="WebHeight" sql-type="smallint" not-null="true" />
    </property>
    <property name="WebWidth">
      <column name="WebWidth" sql-type="smallint" not-null="true" />
    </property>
  </class>
</hibernate-mapping>