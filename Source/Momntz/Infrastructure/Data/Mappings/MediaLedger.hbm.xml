<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="TheMemorableMoments" namespace="TheMemorableMoments.Infrastructure.Data.DTOs" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Media" table="MediaLedger" lazy="true" >
    <id name="MediaLedgerId">
      <generator class="identity" />
      <column name="MediaLedgerId" sql-type="int" not-null="true" />
    </id>
    <property name="Title">
      <column name="Title" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Description">
      <column name="Description" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Tags">
      <column name="Tags" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="MediaYear">
      <column name="MediaYear" sql-type="int" not-null="false" />
    </property>
    <property name="MediaMonth">
      <column name="MediaMonth" sql-type="int" not-null="false" />
    </property>
    <property name="MediaDay">
      <column name="MediaDay" sql-type="int" not-null="false" />
    </property>
    <property name="MediaId">
      <column name="MediaId" sql-type="int" not-null="true" />
    </property>
    <property name="InsertDate">
      <column name="InsertDate" sql-type="datetime2" not-null="true" />
    </property>
    <property name="UserId">
      <column name="UserId" sql-type="int" not-null="true" />
    </property>
    <property name="LocationName">
      <column name="LocationName" sql-type="nvarchar" not-null="false" />
    </property>
  </class>
</hibernate-mapping>